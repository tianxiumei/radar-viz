{
  "name": "radar-viz",
  "version": "1.0.0",
  "description": "可视化应用脚手架",
  "author": "pandora",
  "license": "MIT",
  "repository": "qbox/pandora-visualization",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "start:dev": "rollup -c -w",
    "start": "yarn run start:dev & yarn run start:platform",
    "setup": "yarn install && yarn run setup:platform && yarn run build",
    "setup:platform": "cd platform && yarn install",
    "start:platform": "cd platform && yarn run start",
    "postinstall": "patch-package",
    "package": "yarn run build && cd .. && cp -Rf develop/dist/form.xml appserver/static/visualizations/radar/form.xml && cp -Rf develop/dist/index.es.js appserver/static/visualizations/radar/visualization.js && cp -Rf develop/dist/index.css appserver/static/visualizations/radar/visualization.css && cd .. && tar -czvf radar-viz.tar.gz --exclude=radar-viz/develop radar-viz"
  },
  "peerDependencies": {
    "@qn-pandora/visualization-sdk": "^1.2.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@qn-pandora/visualization-sdk": "^1.2.5",
    "@svgr/rollup": "^2.4.1",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "less": "^3.11.1",
    "patch-package": "^6.2.2",
    "prettier": "^1.13.7",
    "prop-types": "^15.5.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-copy-watch": "^0.0.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "^3.8.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.157",
    "@antv/data-set": "^0.11.4",
    "@antv/g2": "^4.0.12",
    "@antv/g2plot": "^1.1.11",
    "lodash": "^4.17.15"
  }
}
